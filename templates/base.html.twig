<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>ChingYi P.C : {% block title %}{% endblock %}</title>
        <link rel="icon" href={{ asset('build/img/icon.png') }} type="image/png" class='rounded'/>
        <meta name="description" content="ChingYi P.C, développeuse Web PHP/Symfony en recherche d'une opportunité en Île-de-France" />
        <meta name="robots" content="all,follow" />
        <meta charset="utf-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge" />
        <meta name="viewport" content="width=device-width, initial-scale=1" />
        <!-- Google fonts-->
        <link rel="preconnect" href="https://fonts.googleapis.com">
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
        <link href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@400;600&family=Raleway:wght@500;600;700&family=Satisfy&display=swap" rel="stylesheet">    

        {% block stylesheets %}
        <!-- Bootstrap cdn -->
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous"/>
        <!-- Boxicon cdn -->
        <link href="https://unpkg.com/boxicons@2.1.2/css/boxicons.min.css" rel="stylesheet"/>
        <!--animate style cdn-->
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css"/>     
        <!--slide cdn-->
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/glider-js@1/glider.min.css"> 
        <!-- Fotorama -->
        <link  href="https://cdnjs.cloudflare.com/ajax/libs/fotorama/4.6.4/fotorama.css" rel="stylesheet">
        {{ encore_entry_link_tags('app') }}
        {% endblock %}


        {% block javascripts %}
        <!--typeit-->
        <script src="https://unpkg.com/typeit@8.4.0/dist/index.umd.js"></script>
        <!--jquery cdn-->
        <script src="https://code.jquery.com/jquery-3.5.0.js"></script>
        <!-- bootstrap cdn -->
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
        <!--slide cdn-->
        <script src="https://cdn.jsdelivr.net/npm/glider-js@1/glider.min.js"></script>
        <!--fotarama-->
        <script src="https://cdnjs.cloudflare.com/ajax/libs/fotorama/4.6.4/fotorama.js"></script> 
        {{ encore_entry_script_tags('main') }}
        {% endblock %}
    </head>

    <body>
        {% block header %}
        {{ include('header.html.twig') }}
        {% endblock %}
        {% block main %}
        {% endblock %}
        {% block footer %}
        {{ include('footer.html.twig') }}
        {% endblock %}
    </body>
</html>
